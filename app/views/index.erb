<div class="container">
  
  
  <h1>Welcome to Survey Gorilla!</h1>

  <h2>Surveys You Can Take: </h2>
  <ul>
    <% @surveys.each do |survey_obj| %>
      <li><% if survey_obj.uploads != nil %><form action="/surveys/<%= survey_obj.id %>/taken_surveys" method="POST">
        <input type="submit" value="<%= survey_obj.title %>">
      </form>
      </li>
    <% end %>
  </ul>
    
      <p id="error_message"><%= session[:errors]  %></p>
     
  
    
</div>
